    "version": "0.2.0",
    "configurations": [
        {
            "name":"Python: Current File",
            "type":"python",
            "request":"launch",
            "program":"${file}",
            "console":"integratedTerminal"},
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "module": "pyspatialkit"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Chrome",
            "url": "http://localhost:8081/",
            "webRoot": "${workspaceRoot}/frontend/spatialkitcesium/",
            "sourceMaps": true,
            //"sourceMapPathOverrides": {
            //    "webpack:///./*": "${workspaceFolder}/frontend/spatialkitcesium/src/*",
            //    "webpack:///../node_modules/*": "${workspaceFolder}/frontend/spatialkitcesium/node_modules/*",
            //},
        }
    ]
}