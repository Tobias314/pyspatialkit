{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "module": "pyspatialkit"
        },
        {
            "type": "chrome",
            "request": "launch",
            "reAttach": true,
            "name": "frontend",
            "url": "http://localhost:8080/",
            "port": 9222,
            "webRoot": "${workspaceRoot}/frontend/spatialkitcesium/src/app/",
            "sourceMaps": "server",
            //"trace": "verbose",
            //"timeout": 5,
            "pathMappings": [
                {
                    "url": "webpack:///app",
                    "path": "${webRoot}/*"
                }
            ]
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Chrome",
            "url": "http://localhost:8081/",
            "webRoot": "${workspaceRoot}/frontend/spatialkitcesium/",
            "sourceMaps": true,
            //"sourceMapPathOverrides": {
            //    "webpack:///./*": "${workspaceFolder}/frontend/spatialkitcesium/src/*",
            //    "webpack:///../node_modules/*": "${workspaceFolder}/frontend/spatialkitcesium/node_modules/*",
            //},
        }
    ]
}